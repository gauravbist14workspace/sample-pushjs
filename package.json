{
  "name": "sample-pushjs",
  "description": "This project integrated push.js using service workers to get Push notifications on mobile browsers",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "runProd": "http-server -c-1 dist/sample-pushjs --host 0.0.0.0 --port 8080 --ssl --cert ssl/server.crt --key ssl/key.pem",
    "build": "ng build --prod --base-href ./",
    "buildGit": "ng build --prod --base-href https://gauravbist14workspace.github.io/sample-pushjs/",
